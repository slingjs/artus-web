{
  "name": "@sling/artus-web-client",
  "version": "0.0.17",
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./dist && vite build",
    "build:release": "rimraf ./dist && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "keywords": [
    "vue3",
    "typescript"
  ],
  "files": [
    "dist/",
    "package.json"
  ],
  "author": {
    "name": "wangziling",
    "url": "https://github.com/wangziling"
  },
  "repository": {
    "url": "git+https://github.com/slingjs/artus-web.git"
  },
  "homepage": "https://github.com/slingjs/artus-web",
  "dependencies": {
    "@sling/artus-web-shared": "workspace:^",
    "@vicons/carbon": "^0.12.0",
    "js-cookie": "^3.0.1",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/js-cookie": "^3.0.3",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.61.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  }
}
